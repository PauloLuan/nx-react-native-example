{
  "version": 2,
  "projects": {
    "bible": {
      "root": "apps/bible",
      "sourceRoot": "apps/bible/src",
      "projectType": "application",
      "generators": {},
      "targets": {
        "bundle": {
          "executor": "nx-react-native-expo:bundle",
          "options": {}
        },
        "start": {
          "executor": "nx-react-native-expo:start",
          "options": {}
        },
        "run-ios": {
          "executor": "nx-react-native-expo:run-ios",
          "options": {}
        },
        "run-android": {
          "executor": "nx-react-native-expo:run-android",
          "options": {}
        },
        "run-web": {
          "executor": "nx-react-native-expo:run-web",
          "options": {}
        },
        "publish": {
          "executor": "nx-react-native-expo:publish",
          "options": {}
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/bible/**/*.{js,ts,tsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/bible"
          ],
          "options": {
            "jestConfig": "apps/bible/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "defaultProject": "bible"
}